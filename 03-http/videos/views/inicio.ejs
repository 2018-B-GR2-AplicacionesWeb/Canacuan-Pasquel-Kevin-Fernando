<style>
    table, th, td {
        border: 1px solid yellowgreen;
    }
    button {
        border: 1px solid cornflowerblue;
    }
</style>


<!-- codigo js -->
<%var apellido = 'Canacuan'%>

<!-- = sirve para mostrar datos en pantalla-->
<h1> <%=nombre%> <%=apellido%> </h1>
<p>Parrafo</p>

<!-- LISTAS -->
<!-- ul -> unordered list -->
<!-- ol -> ordered list -->
<!-- li -> list item -->

<h2>Arreglo</h2>

<!-- <ul>
<% arreglo.forEach((numero) => { %>
        <li> <%= numero %> </li>
<% }) %>
</ul> -->

<!-- TABLAS -->
<!-- tr -> table row -->
<!-- th -> table header -->
<!-- td -> table detail-->
<table>
    <tr>
        <th>Identificador</th>
        <th>Nombre</th>
        <th>Acciones</th>
    </tr>
    <% arreglo.forEach((usuario) => { %>
    <tr>
        <td>
            <%= usuario.id  %>
        </td>
        <td>
            <%= usuario.nombre  %>
        </td>
        <td>
            <form action="/Usuario/borrar/<%= usuario.id  %>"
                  method="post">
                <button type="button">Yo no</button>
                <button>Yo si</button>
                <button type="submit">Eliminar</button>
            </form>
        </td>
    </tr>
    <% })%>
</table>